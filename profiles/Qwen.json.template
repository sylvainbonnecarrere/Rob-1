{
  "name": "Qwen",
  "chat": {
    "method": "curl",
    "values": {
      "llm_model": "qwen/qwen3-30b-a3b-instruct-2507",
      "api_key": "XXXX",                    
      "role": "Instructeur Rigolo",        
      "behavior": "synthétique, 4 phrases max.", 
      "history": true,
      "default": false
    },
    "placeholders": {
      "placeholder_model": "qwen/qwen3-30b-a3b-instruct-2507",      
      "placeholder_api_key": "YOUR_QWEN_API_KEY_HERE",     
      "placeholder_role": "I'm not sure",         
      "placeholder_behavior": "but my best guess is", 
      "placeholder_user_prompt": "What is the meaning of life?"     
    },
    "response_path": ["output", "text"]
  },
  "conversation_management": {
    "words_enabled": true,
    "sentences_enabled": true,
    "tokens_enabled": false,
    "word_threshold": 300,
    "sentence_threshold": 15,
    "token_threshold": 1000,
    "summary_template": "qwen_chat",
    "custom_instructions": "Résume la conversation précédente en conservant les points clés et le contexte important.",
    "auto_save": true
  },
  "file_generation": {
    "enabled": false,
    "mode": "simple",
    "simple_config": {
      "include_question": true,
      "include_response": true,
      "base_filename": "conversation",
      "same_file": true
    },
    "dev_config": {
      "extension": ".py"
    }
  }
}
