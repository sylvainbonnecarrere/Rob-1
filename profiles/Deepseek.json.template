{
  "name": "Deepseek",
  "api_url": "https://openrouter.ai/api/v1/chat/completions",
  "api_key": "YOUR_OPENROUTER_API_KEY_HERE",
  "role": "",
  "behavior": "",
  "history": true,
  "default": false,
  "replace_apikey": "OPENROUTER_API_KEY",
  "template_id": "deepseek_chat",
  "method": "curl",
  "template_type": "chat",
  "llm_model": "deepseek-chat",
  "response_path": ["choices", 0, "message", "content"],
  "conversation_management": {
    "words_enabled": true,
    "sentences_enabled": true,
    "tokens_enabled": false,
    "word_threshold": 300,
    "sentence_threshold": 10,
    "token_threshold": 1000,
    "summary_template": "Template Deepseek",
    "custom_instructions": "Résume la conversation précédente en conservant les points clés et le contexte important.",
    "auto_save": true
  },
  "conversation": {
    "enabled": true,
    "max_context_length": 4000,
    "summary_enabled": true,
    "summary_trigger_length": 8000,
    "summary_target_length": 2000,
    "summary_template_id": "deepseek_summary",
    "conversation_file": "conversations/conversation.txt"
  },
  "file_generation": {
    "enabled": false,
    "mode": "simple",
    "simple_config": {
      "include_question": true,
      "include_response": true,
      "base_filename": "conversation",
      "same_file": true
    },
    "dev_config": {
      "extension": ".py"
    }
  }
}
